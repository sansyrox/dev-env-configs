{
	"name": "vscode-wakatime",
	"displayName": "WakaTime",
	"description": "Metrics, insights, and time tracking automatically generated from your programming activity.",
	"version": "1.3.0",
	"publisher": "WakaTime",
	"author": {
		"name": "WakaTime"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"productivity",
		"time tracking",
		"project",
		"git",
		"timer"
	],
	"homepage": "https://wakatime.com/",
	"bugs": {
		"url": "https://github.com/wakatime/vscode-wakatime/issues",
		"email": "support@wakatime.com"
	},
	"license": "BSD-3-Clause",
	"icon": "images/wakatime-logo.png",
	"galleryBanner": {
		"color": "#AEC3D8",
		"theme": "light"
	},
	"activationEvents": [
		"*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wakatime/vscode-wakatime.git"
	},
	"engines": {
		"vscode": "^1.17.0",
		"sqlops": "*"
	},
	"main": "./dist/extension",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"compile-dev": "tsc -p ./",
		"watch": "webpack --mode none --watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile-dev && node ./node_modules/vscode/bin/test"
	},
	"contributes": {
		"commands": [
			{
				"command": "wakatime.apikey",
				"title": "WakaTime: Api Key"
			},
			{
				"command": "wakatime.proxy",
				"title": "WakaTime: Set Proxy"
			},
			{
				"command": "wakatime.debug",
				"title": "WakaTime: Set Debug"
			},
			{
				"command": "wakatime.status_bar_icon",
				"title": "WakaTime: Status Bar Icon"
			},
			{
				"command": "wakatime.dashboard",
				"title": "WakaTime: Open Dashboard"
			},
			{
				"command": "wakatime.config_file",
				"title": "WakaTime: Open Config File..."
			},
			{
				"command": "wakatime.log_file",
				"title": "WakaTime: Open Log File..."
			}
		]
	},
	"devDependencies": {
		"@types/adm-zip": "^0.4.32",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.13.4",
		"@types/request": "^2.48.1",
		"@types/rimraf": "^2.0.2",
		"@types/webpack": "^4.4.27",
		"adm-zip": "0.4.13",
		"del-cli": "^1.1.0",
		"original-fs": "^1.0.0",
		"request": "2.88.0",
		"rimraf": "^2.6.3",
		"ts-loader": "^5.3.3",
		"tslib": "^1.9.3",
		"typescript": "^3.4.3",
		"vscode": "^1.1.33",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.0"
	},
	"__metadata": {
		"id": "f2fd462f-f1bd-4c62-b54f-59a4e5ffc6a3",
		"publisherId": "27977793-9e84-451b-8977-744ce63f3d70",
		"publisherDisplayName": "WakaTime"
	}
}